[general]
default-layout='krzystof-tweak'
object-id-list=['mate-menu', 'notification-area', 'clock', 'window-list', 'workspace-switcher']
toplevel-id-list=['top', 'toplevel-0']

[toplevels/top]
expand=true
orientation='top'
screen=0
size=28

[toplevels/top/background]
opacity=65535
color='#2e3436'
type='color'

[toplevels/toplevel-0]
orientation='bottom'
screen=0
monitor=0
y=1056
y-bottom=0

[toplevels/toplevel-0/background]
opacity=65535
color='#2e3436'
type='color'

[objects/clock]
applet-iid='ClockAppletFactory::ClockApplet'
locked=true
toplevel-id='top'
position=138
object-type='applet'
panel-right-stick=true

[objects/clock/prefs]
expand-locations=false
format='24-hour'
custom-format=''

[objects/notification-area]
applet-iid='NotificationAreaAppletFactory::NotificationArea'
locked=true
toplevel-id='top'
position=391
object-type='applet'
panel-right-stick=true

[objects/workspace-switcher]
applet-iid='WnckletFactory::WorkspaceSwitcherApplet'
locked=true
toplevel-id='toplevel-0'
position=321
object-type='applet'
panel-right-stick=true

[objects/workspace-switcher/prefs]
display-workspace-names=false
wrap-workspaces=false
display-all-workspaces=true
num-rows=1

[objects/mate-menu]
applet-iid='MateMenuAppletFactory::MateMenuApplet'
locked=true
toplevel-id='top'
position=0
object-type='applet'

[objects/window-list]
applet-iid='WnckletFactory::WindowListApplet'
locked=false
toplevel-id='toplevel-0'
position=0
object-type='applet'
